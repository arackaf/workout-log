    "easy-express-controllers": "0.2.1",
    "express": "4.x",
    "express-session": "1.12.1",
    "mobx": "3.1.7",
    "mobx-react": "4.1.3",
    "mongodb": "2.0.39",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "passport-remember-me": "0.0.1",
    "react": "15.4.2",
    "react-addons-css-transition-group": "15.4.2",    
    "react-collapse": "2.3.3",
    "react-dom": "15.4.2",
    "react-height": "2.1.1",
    "react-motion": "0.4.7",    
